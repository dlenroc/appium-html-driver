{
  "name": "@dlenroc/appium-html-driver",
  "version": "0.0.3",
  "description": "WebDriver for testing HTML applications",
  "author": "Corneliu Duplachi",
  "repository": "dlenroc/appium-html-driver",
  "keywords": [
    "html",
    "appium",
    "webdriver"
  ],
  "license": "MIT",
  "main": "dist/server.js",
  "browser": "dist/client.js",
  "types": "src/server/index.ts",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "NODE_OPTIONS=--max_old_space_size=6144 rollup -c",
    "test": "npm run test:odc && npm run test:cdp",
    "test:odc": "MODE=ODC mocha -r ts-node/register -b -t 5000 test/**/*.ts",
    "test:cdp": "MODE=CDP mocha -r ts-node/register -b -t 5000 test/**/*.ts"
  },
  "engines": {
    "node": ">=18"
  },
  "appium": {
    "driverName": "html",
    "automationName": "html",
    "platformNames": [
      "html"
    ],
    "mainClass": "Driver"
  },
  "dependencies": {
    "@appium/base-driver": "^9.4.4",
    "chrome-remote-interface": "^0.33.0",
    "express": "^4.18.2",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "@appium/types": "^0.15.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/chai": "^4.3.11",
    "@types/chai-as-promised": "^7.1.8",
    "@types/chai-like": "^1.1.3",
    "@types/chai-things": "^0.0.38",
    "@types/chrome-remote-interface": "^0.31.14",
    "@types/cssesc": "^3.0.2",
    "@types/express": "^4.17.21",
    "@types/lodash-es": "^4.17.12",
    "@types/mocha": "^10.0.6",
    "chai": "^4.3.10",
    "chai-as-promised": "^7.1.1",
    "chai-like": "^1.1.1",
    "chai-things": "^0.2.0",
    "cssesc": "^3.0.0",
    "lodash-es": "^4.17.21",
    "mocha": "^10.2.0",
    "playwright-chromium": "^1.40.1",
    "rollup": "^4.9.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "scroll-into-view-if-needed": "^3.1.0",
    "socket.io-client": "^4.7.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1",
    "webdriverio": "^8.27.0"
  },
  "peerDependencies": {
    "appium": "^2.3.0"
  }
}
