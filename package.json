{
  "name": "@dlenroc/appium-html-driver",
  "version": "0.1.0",
  "description": "WebDriver for testing HTML applications",
  "author": "Corneliu Duplachi",
  "repository": "dlenroc/appium-html-driver",
  "keywords": [
    "html",
    "appium",
    "webdriver"
  ],
  "license": "MIT",
  "main": "dist/server.js",
  "browser": "dist/client.js",
  "types": "src/server/index.ts",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "NODE_OPTIONS=--max_old_space_size=6144 rollup -c",
    "watch": "rollup -c -w",
    "prepare": "npm run build",
    "test": "mocha -r ts-node/register -b -p -j 3 -t 10000 --retries 1 test/**/*.ts"
  },
  "engines": {
    "node": ">=12"
  },
  "appium": {
    "driverName": "html",
    "automationName": "html",
    "platformNames": [
      "html"
    ],
    "mainClass": "Driver"
  },
  "dependencies": {
    "@appium/base-driver": "^8.5.7",
    "chrome-remote-interface": "^0.31.2",
    "express": "^4.18.1",
    "socket.io": "^4.5.1"
  },
  "devDependencies": {
    "@appium/types": "^0.2.5",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/chai": "^4.3.1",
    "@types/chai-as-promised": "^7.1.5",
    "@types/chai-like": "^1.1.1",
    "@types/chai-things": "^0.0.35",
    "@types/chrome-remote-interface": "^0.31.4",
    "@types/cssesc": "^3.0.0",
    "@types/express": "^4.17.13",
    "@types/lodash-es": "^4.17.6",
    "@types/mocha": "^9.1.1",
    "@types/uuid": "^8.3.4",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "chai-like": "^1.1.1",
    "chai-things": "^0.2.0",
    "cssesc": "^3.0.0",
    "lodash-es": "^4.17.21",
    "mocha": "^10.0.0",
    "playwright-chromium": "^1.22.2",
    "rollup": "^2.75.6",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.32.1",
    "scroll-into-view-if-needed": "^2.2.29",
    "socket.io-client": "^4.5.1",
    "ts-node": "^10.8.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.3",
    "uuid": "^8.3.2",
    "webdriverio": "^7.20.2"
  }
}
